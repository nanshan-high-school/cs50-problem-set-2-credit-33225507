#include <iostream>
using namespace std;
int main() {
    long int card;
    cout << "請輸入您的信用卡卡號:";
    cin >> card;
    int a = card / 1000000000000000;
    int y = a;
    int b = card % 1000000000000000 / 100000000000000;
    int c = card % 1000000000000000 % 100000000000000 / 10000000000000;
    int d = card % 1000000000000000 % 100000000000000 % 10000000000000 / 1000000000000;
    int e = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 / 100000000000;
    int f = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 / 10000000000;
    int g = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 / 1000000000;
    int h = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 / 100000000;
    int i = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 / 10000000;
    int j = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 % 10000000 / 1000000;
    int k = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 % 10000000 % 1000000 / 100000;
    int l = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 % 10000000 % 1000000 % 100000 / 10000;
    int m = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 % 10000000 % 1000000 % 100000 % 10000 / 1000;
    int n = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 % 10000000 % 1000000 % 100000 % 10000 % 1000 / 100;
    int o = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 % 10000000 % 1000000 % 100000 % 10000 % 1000 % 100 / 10;
    int p = card % 1000000000000000 % 100000000000000 % 10000000000000 % 1000000000000 % 100000000000 % 10000000000 % 1000000000 % 100000000 % 10000000 % 1000000 % 100000 % 10000 % 1000 % 100 % 10;
    if (a > 4) {
       a = a * 2 / 10 + a * 2 % 10;
    }
    else {
        a = a * 2;
    }
    if (c > 4) {
       c = c * 2/ 10 + c * 2 % 10;
    }
    else {
        c = c * 2;
    }
    if (e > 4) {
       e = e * 2 / 10 + e * 2 % 10;
    }
    else {
        e = e * 2;
    }
    if (g > 4) {
       g = g * 2 / 10 + g * 2 % 10;
    }
    else {
        g = g * 2;
    }
    if (i > 4) {
       i = i * 2 / 10 + i * 2 % 10;
    }
    else {
        i = i * 2;
    }
    if (k > 4) {
       k = k * 2 / 10 + k * 2 % 10;
    }
    else {
        k = k * 2;
    }
    if (m > 4) {
       m = m * 2 / 10 + m * 2 % 10;
    }
    else {
        m = m * 2;
    }
    if (o > 4) {
       o = o * 2 / 10 + o * 2 % 10;
    }
    else {
        o = o * 2;
    }
    int x = a + b + c + d + e + f + g + h + i + j + k + l + m + n + o;
    if (x % 10 == 0) {
       x = 0;
    }
    else {
        x = 10 - x % 10;
    }
    if (x == p) {
       if (y == 4) { 
          cout << "您的信用卡為Visa的\n";
       } 
       else if (y == 5) { 
       cout << "您的信用卡為Master的\n";
       }  
       else {
       cout << "您的信用卡卡號無效\n";
       }
    }
    else {
        cout << "您的信用卡卡號無效\n";
    }
}
